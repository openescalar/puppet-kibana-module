NameVirtualHost *:80

<VirtualHost *:80>
    ServerName na1kibana.bio-iad.ea.com
    DocumentRoot /opt/kibana/v3
    ServerAlias auscontrolplugin01
    ErrorLog logs/error_log
    CustomLog logs/access_log common
        ProxyRequests Off
        ProxyPreserveHost On

    RewriteEngine On
    # Rewrite all requests to use port 443, preserve the host name
    # so the rewrites in the below vhost include will work correctly
    #RewriteRule ^/(.*)            https://10.72.173.6/$1 [L,R,NE]
    #RewriteRule ^!/es(.*)            https://na1kibana.bio-iad.ea.com/$1 [L,R,NE]

  # Set global proxy timeouts
  <Proxy http://10.72.173.5:9200>
    ProxySet connectiontimeout=5 timeout=90
  </Proxy>
 
  # Proxy for _aliases and .*/_search
  <LocationMatch "^(/_aliases|.*/_search|.*/_mapping)$">
    ProxyPassMatch http://10.72.173.5:9200
    ProxyPassReverse http://10.72.173.5:9200
  </LocationMatch>
 
  # Proxy for kibana-int/{dashboard,temp} stuff (if you don't want auth on /, then you will want these to be protected)
  <LocationMatch "^(/kibana-int/dashboard/|/kibana-int/temp).*$">
    ProxyPassMatch http://10.72.173.5:9200
    ProxyPassReverse http://10.72.173.5:9200
  </LocationMatch>


</VirtualHost>
